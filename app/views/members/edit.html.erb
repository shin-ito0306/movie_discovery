<%= render 'layouts/error_message', object: @member %>
<%= form_with model:@member, local:true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label :introduction %>
  <%= f.text_area :introduction %>
  
  <%= f.label :member_image %>
  <%= f.attachment_field :member_image %>
  
  <%= f.submit %>
  
<% end %>